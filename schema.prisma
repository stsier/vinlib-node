// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url      = env("DATABASE_URL")
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id       String  @id @default(cuid())
  name     String?
  email    String? @unique
  password String?
  wines    Wine[]  @relation("Wine_author")
}

model Wine {
  id              String    @id @default(cuid())
  title           String?
  photo_filesize  Int?
  photo_extension String?
  photo_width     Int?
  photo_height    Int?
  photo_mode      String?
  photo_id        String?
  sift_filesize   Int?
  sift_mode       String?
  sift_filename   String?
  status          String?
  content         String?
  publishDate     DateTime?
  author          User?     @relation("Wine_author", fields: [authorId], references: [id])
  authorId        String?   @map("author")
  tags            Tag[]     @relation("Tag_wines_Wine_tags")

  @@index([authorId])
}

model Tag {
  id    String  @id @default(cuid())
  name  String?
  wines Wine[]  @relation("Tag_wines_Wine_tags")
}